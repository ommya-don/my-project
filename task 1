a= float(input("Enter the first number :" ))
b=float(input("Enter the secound number  :"))

addition =a+b
subtraction = a-b
multiplication =a*b
division =a/b

if b !=0  :
    division=a/b
else :
    print ("indefine")


print(addition)
print(subtraction)
print(multiplication)
print(division)

